import java.io.*;    
public class TestClass {
     public static void main(String[] args) throws IOException {
         try{
              BufferedReader bf = new TestClass("in.txt");
        BufferedWriter bw = new TestClass("Buff.txt");
        String line;
        while((line = bf.readLine()) != null) {
              bw.write(line);
                    bw.newLine();
              }
                bw.flush();
                } catch(FileNotFoundException fnfe){                         }
           }
}